{
  "name": "protocol-pro-monorepo",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "mobile": "cd apps/mobile && npm run start"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "prettier": "^3.2.5",
    "turbo": "^2.4.4"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "npm@10.2.3"
} 